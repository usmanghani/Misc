PROCEDURE DP
PUBLIC DP
DP='D'
DEFINE WINDOW W1 AT 1, 20 SIZE 5, 60 TITLE 'Select';
FONT "MS SANS SERIF",8;
COLOR RGB(,,,192,192,192)
ACTIVATE WINDOW W1
MOVE WINDOW W1 CENTER
@ 1,7 SAY 'Display Print   [D   P   F] ' FONT "ARIAL",12 STYLE "BT"
@ 1,50 GET dp PICTURE '!' VALID dp$'DPFdpf' FONT "ARIAL",12 STYLE "BT"
READ
RELE WIND W1
RETURN DP

